# gcd of strings

class Solution {
public:
    int findgcd(int a, int b){
		if(b == 0) return a;
		return findgcd(b,a%b);
	}

	string gcdOfStrings(string str1, string str2) {
		if(str1 + str2 != str2 + str1) return "";
		int gcd = findgcd(str1.length(), str2.length()); gcd(6,3)
		return str1.substr(0,gcd);
	}
};
tc=o(n)
